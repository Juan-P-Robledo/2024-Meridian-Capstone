    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>This Client Script will get a URL from an API and display the image in the corresponding spot. </description>
        <field/>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Display Show Image</name>
        <order/>
        <script><![CDATA[function onLoad() {
    try {
        // Function to set image source and handle errors
        function setImageSource(imageUrl) {
            try {
                var imgElement = document.getElementById('showImage');
                if (imgElement) {
                    imgElement.src = imageUrl;
                    g_form.addInfoMessage('Image URL set successfully: ' + imageUrl);
                } else {
                    g_form.addWarningMessage('Element with id "showImage" not found.');
                }
            } catch (error) {
                g_form.addErrorMessage('Error setting image URL: ' + error.message);
            }
        }

        // Check if g_form and document are defined and ready
        if (typeof g_form !== 'undefined' && document.readyState === 'complete') {
            var imageUrl = g_form.getValue('u_url');
            if (imageUrl) {
                setImageSource(imageUrl);
            } else {
                g_form.addInfoMessage('No URL found in u_url field');
            }
        } else {
            // Retry after a short delay if not ready
            setTimeout(displayImageOnLoad, 100);
        }
    } catch (error) {
        g_form.addErrorMessage('Error in script: ' + error.message);
    }
}

// Call the function when the form is fully loaded
document.addEventListener('DOMContentLoaded', function() {
    displayImageOnLoad();
});
]]></script>
